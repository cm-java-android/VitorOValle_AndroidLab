<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_quarta_tela"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.principal.sided.QuartaTelaActivity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true">

        <TextView
            android:text="Progresso das aulas"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="36dp"
            android:id="@+id/CaixaTexto4" />

        <TextView
            android:text="Laboratórios enviados ao Github"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/CaixaTexto6"
            android:layout_below="@+id/ProgressoExercicio"
            android:layout_alignStart="@+id/ProgressoExercicio"
            android:layout_marginTop="19dp" />

        <TextView
            android:text="Aulas dadas"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="49dp"
            android:id="@+id/CaixaTexto5"
            android:layout_below="@+id/CaixaTexto4"
            android:layout_alignStart="@+id/ProgressoAulas" />

        <ProgressBar
            style="@android:style/Widget.ProgressBar.Horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="visible"
            android:max="3"
            android:minWidth="300dp"
            android:progress="1"
            android:id="@+id/ProgressoAulas"
            android:layout_marginTop="29dp"
            android:layout_below="@+id/CaixaTexto5"
            android:layout_alignStart="@+id/CaixaTexto6" />

        <TextView
            android:text="Exercícios feitos"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/CaixaTexto6"
            android:layout_below="@+id/ProgressoAulas"
            android:layout_alignStart="@+id/ProgressoExercicio"
            android:layout_marginTop="36dp" />

        <ProgressBar
            style="@android:style/Widget.ProgressBar.Horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="visible"
            android:max="3"
            android:minWidth="300dp"
            android:progress="1"
            android:id="@+id/ProgressoExercicio"
            android:layout_below="@+id/CaixaTexto6"
            android:layout_alignStart="@+id/ProgressoLabGit"
            android:layout_marginTop="26dp" />

        <Button
            android:text="Voltar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            android:layout_marginBottom="54dp"
            android:id="@+id/BotaoVoltar3" />

        <ProgressBar
            style="@android:style/Widget.ProgressBar.Horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="23dp"
            android:visibility="visible"
            android:max="3"
            android:minWidth="300dp"
            android:progress="1"
            android:id="@+id/ProgressoLabGit"
            android:layout_above="@+id/BotaoVoltar3"
            android:layout_centerHorizontal="true" />


    </RelativeLayout>
</RelativeLayout>
